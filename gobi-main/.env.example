# Database
DATABASE_URL="postgresql://username:password@localhost:5432/database_name?schema=public"

# Server Configuration
PORT=3000
NODE_ENV=development

# JWT Configuration - Generate your own RSA key pair
JWT_PUBLIC_KEY="-----BEGIN PUBLIC KEY-----
YOUR_PUBLIC_KEY_HERE
-----END PUBLIC KEY-----"

JWT_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----
YOUR_PRIVATE_KEY_HERE
-----END PRIVATE KEY-----"

# CORS Configuration
CORS_ORIGIN="http://localhost:3000,http://localhost:3001,http://localhost:3026,http://localhost:3027"

# Twilio Configuration
TWILIO_ACCOUNT_SID=ACxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
TWILIO_AUTH_TOKEN=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# LiveKit Configuration
LIVEKIT_API_KEY=APIxxxxxxxxxxxxxxx
LIVEKIT_API_SECRET=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
LIVEKIT_SERVER_URL=https://your-app.livekit.cloud
LIVEKIT_SIP_URL=sip:your-sip-endpoint.sip.livekit.cloud

# Logging
LOG_LEVEL=info